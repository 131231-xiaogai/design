<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="weekIndicatorStyle">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">#ff25adff</item>
    </style>

    <style name="mybutton">
        <item name="android:background">@drawable/button_bg</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">13sp</item>
        <item name="android:layout_margin">10dp</item>
        <item name="android:textColor">#6A5ACD</item>

    </style>

    <style name="my_order_ui">
        <item name="android:background">@drawable/shape_h_tubiao</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">30sp</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:layout_marginBottom">20dp</item>

    </style>
    <style name="sk_ui">
        <item name="android:background"></item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">20sp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginBottom">10dp</item>

    </style>

    <style name="m_line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_marginTop">4dp</item>
        <item name="android:layout_height">38dp</item>
        <item name="android:background"> @drawable/shape_m_line</item>
        <item name="android:gravity">center </item>
        <item name="android:textSize">20sp</item>

    </style>

    <style name="list_item_image">
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_width">65dp</item>
        <item name="android:layout_height">65dp</item>

    </style>
    <style name="list_item_text">
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_width">65dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">19sp</item>

        <item name="android:textColor">#6A5ACD</item>
    </style>



</resources>
